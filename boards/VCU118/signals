FIREFLY_MODSEL_LS_B                 inout
FIREFLY_RESET_LS_B                  inout
FIREFLY_{RX,TX}_{P,N}[1..4]         inout

# FMC+ connector with 24 MGT links
FMCP_HSPC_CLK{0,1}_M2C_{P,N}        inout
FMCP_HSPC_DP_{C2M,M2C}_{P,N}[0..23] inout
FMCP_HSPC_GBT0_0_{P,N}              inout
FMCP_HSPC_GBT0_1_{P,N}              inout
FMCP_HSPC_GBT1_0_{P,N}              inout
FMCP_HSPC_GBT1_1_{P,N}              inout
