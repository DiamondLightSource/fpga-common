FMCP_HSPC_GBT1_3_{P,N}              inout
FMCP_HSPC_GBT1_4_{P,N}              inout
FMCP_HSPC_GBT1_5_{P,N}              inout
FMCP_HSPC_GBTCLK2_M2C_C_{P,N}       inout
FMCP_HSPC_GBTCLK3_M2C_C_{P,N}       inout
FMCP_HSPC_GBTCLK4_M2C_C_{P,N}       inout
FMCP_HSPC_GBTCLK5_M2C_C_{P,N}       inout
FMCP_HSPC_H_PRSNT_M2C_B_LS          inout
FMCP_HSPC_PG_M2C_LS                 inout
FMCP_HSPC_REFCLK_{C2M,M2C}_{P,N}    inout
FMCP_HSPC_SYNC_{C2M,M2C}_{P,N}      inout
FMCP_HSPC_Z_PRSNT_M2C_B_LS          inout
FMCP_HSPC_HA_{P,N}[0..23]           inout
FMCP_HSPC_LA_{P,N}[0..33]           inout

# FMC connector
FMC_HPC1_CLK{0,1}_M2C_{P,N}         inout
